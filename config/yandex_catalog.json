{
  "page_generators": [
    "google", "sitemap", "site-scraping"
  ],
  "distribution_types": [
    "uniform", "log-normal"
  ],
  "schemes": {
    "default": {
      "page_generator": "site-scraping",
      "time_between_page": {
        "type": "uniform",
        "low_boundary": 1,
        "up_boundary": 15
      },
      "page_number":{
        "type": "log-normal",
        "M": 2,
        "D": 1
      }
    },
    "fast_going": {
      "page_generator": "site-scraping",
      "time_between_page": {
        "type": "dual-normal",
        "M1": 10,
        "D1": 40,
        "M2": 70,
        "D2": 500
      },
      "page_number":{
        "type": "uniform",
        "low_boundary": 1,
        "up_boundary": 10
      }
    }
  },
  "sites":[
    {
      "type": "loaded_list",
      "file_type": "xlsx",
      "file_name": "support_files/sitelist.xlsx",
      "worksheet":"List",
      "column":"A",
      "count_for_visit": 0,
      "scheme": "fast_going"
    }
  ]
}